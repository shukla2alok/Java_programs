public class swap {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 5;
		int b = 15;
		System.out.println("Numbers before swapping: a = "+a+", b = "+b);
		a = a+b;
		b = a-b;
		a = a-b;
		System.out.println("Numbers after swapping: a = "+a+", b = "+b);
	}
}
